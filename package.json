{
    "name": "obsidian-emera",
    "version": "1.4.4",
    "description": "MDX for Obsidian. Kinda.",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "typecheck": "tsc -noEmit -skipLibCheck",
        "build": "yarn typecheck && node esbuild.config.mjs production",
        "test": "vitest run",
        "lint": "eslint . --max-warnings=0",
        "format": "prettier --check .",
        "format:write": "prettier --write .",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "hooks:install": "node scripts/install-git-hooks.mjs",
        "sync:vault": "node --env-file-if-exists=.env.local scripts/deploy-to-vault.mjs",
        "deploy:vault": "yarn build && yarn sync:vault"
    },
    "keywords": [],
    "author": "OlegWock & steven-ahfu",
    "license": "AGPL-3.0-only",
    "engines": {
        "node": ">=24.13.1"
    },
    "devDependencies": {
        "@types/babel__standalone": "^7.1.9",
        "@types/node": "^20.19.22",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "@typescript-eslint/eslint-plugin": "^8.56.0",
        "@typescript-eslint/parser": "^8.56.0",
        "builtin-modules": "^5.0.0",
        "esbuild": "^0.27.3",
        "eslint": "^10.0.0",
        "eslint-config-prettier": "^10.1.8",
        "obsidian": "^1.12.0",
        "prettier": "^3.8.1",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "@babel/standalone": "^7.29.1",
        "@codemirror/lang-javascript": "^0.20.0",
        "@codemirror/lang-markdown": "^0.20.0",
        "@codemirror/language-data": "^0.20.0",
        "@emotion/is-prop-valid": "^1.2.2",
        "@rollup/browser": "^4.57.1",
        "@ungap/with-resolvers": "^0.1.0",
        "eventemitter3": "^5.0.4",
        "framer-motion": "^12.34.1",
        "jotai": "^2.17.1",
        "nanoid": "^5.1.6",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "react-error-boundary": "^6.1.1",
        "react-merge-refs": "^3.0.2"
    }
}
